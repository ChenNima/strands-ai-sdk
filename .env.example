# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/strands_ai

# Alternative format for some libraries
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=strands_ai

# AWS Credentials (for Bedrock)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-west-2

# OIDC Configuration
# The issuer URL - all other endpoints will be auto-discovered from .well-known/openid-configuration
NEXT_PUBLIC_OIDC_ISSUER=https://your-oidc-provider.com
NEXT_PUBLIC_OIDC_CLIENT_ID=your_client_id
