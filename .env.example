# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/strands_ai

# AWS Credentials (for Bedrock)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-west-2

# OIDC Configuration
# The issuer URL - all other endpoints will be auto-discovered from .well-known/openid-configuration
# Backend (FastAPI) reads these directly, frontend (Next.js) gets them via next.config.js mapping
OIDC_ISSUER=https://your-oidc-provider.com
OIDC_CLIENT_ID=your_client_id

# API KEY for Zhipu MCP. Get your key from https://www.bigmodel.cn/usercenter/proj-mgmt/apikeys
# ZHIPU_API_KEY=xxx